{"kind":"symbol","hierarchy":{"paths":[["doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore","doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig","doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct"]]},"sections":[],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct\/allowsuturnatwaypoint"]}],"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct\/allowsUTurnAtWaypoint"},"primaryContentSections":[{"kind":"declarations","declarations":[{"platforms":["iOS"],"languages":["swift"],"tokens":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"allowsUTurnAtWaypoint","kind":"identifier"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:Sb","text":"Bool","kind":"typeIdentifier"}]}]},{"kind":"content","content":[{"level":2,"anchor":"discussion","text":"Discussion","type":"heading"},{"type":"paragraph","inlineContent":[{"text":"If the value of this property is ","type":"text"},{"type":"codeVoice","code":"true"},{"text":", a returned route may require an immediate U-turn at an","type":"text"},{"text":" ","type":"text"},{"text":"intermediate","type":"text"},{"type":"text","text":" "},{"text":"waypoint. At an intermediate waypoint, if the value of this property is ","type":"text"},{"type":"codeVoice","code":"false"},{"type":"text","text":", each returned route may"},{"type":"text","text":" "},{"type":"text","text":"continue straight ahead or turn to either side but may not U-turn. This property has no effect if only two"},{"type":"text","text":" "},{"type":"text","text":"waypoints are specified."}]},{"type":"paragraph","inlineContent":[{"text":"Set this property to ","type":"text"},{"type":"codeVoice","code":"true"},{"text":" if you expect the user to traverse each leg of the trip separately. For","type":"text"},{"text":" ","type":"text"},{"text":"example, it","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"would be quite easy for the user to effectively “U-turn” at a waypoint if the user first parks the car and"},{"type":"text","text":" "},{"text":"patronizes a restaurant there before embarking on the next leg of the trip. Set this property to ","type":"text"},{"type":"codeVoice","code":"false"},{"text":" if","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"you"},{"text":" ","type":"text"},{"text":"expect the user to proceed to the next waypoint immediately upon arrival. For example, if the user only","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"needs to"},{"text":" ","type":"text"},{"text":"drop off a passenger or package at the waypoint before continuing, it would be inconvenient to perform a","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"U-turn"},{"text":" ","type":"text"},{"type":"text","text":"at that location."},{"type":"text","text":" "},{"text":"The default value of this property is `false.","type":"text"}]}]}],"schemaVersion":{"patch":0,"major":0,"minor":3},"metadata":{"role":"symbol","modules":[{"name":"MapboxNavigationCore"}],"title":"allowsUTurnAtWaypoint","roleHeading":"Instance Property","symbolKind":"property","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"allowsUTurnAtWaypoint","kind":"identifier"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Bool","preciseIdentifier":"s:Sb"}],"externalID":"s:20MapboxNavigationCore0C6ConfigV012RouteRequestD0V21allowsUTurnAtWaypointSbvp"},"abstract":[{"type":"text","text":"A Boolean value that indicates whether a returned route may require a point U-turn at an intermediate"},{"type":"text","text":" "},{"text":"waypoint.","type":"text"}],"references":{"doc://MapboxNavigationCore/documentation/MapboxNavigationCore/CoreConfig":{"kind":"symbol","type":"topic","url":"\/documentation\/mapboxnavigationcore\/coreconfig","role":"symbol","navigatorTitle":[{"text":"CoreConfig","kind":"identifier"}],"identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig","title":"CoreConfig","abstract":[{"type":"text","text":"Mutable Core SDK configuration."}],"fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"CoreConfig"}]},"doc://MapboxNavigationCore/documentation/MapboxNavigationCore/CoreConfig/RouteRequestConfig-swift.struct":{"kind":"symbol","url":"\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct","fragments":[{"text":"struct","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"RouteRequestConfig"}],"title":"CoreConfig.RouteRequestConfig","role":"symbol","identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct","navigatorTitle":[{"text":"RouteRequestConfig","kind":"identifier"}],"abstract":[{"type":"text","text":"Mutable Routing configuration."}],"type":"topic"},"doc://MapboxNavigationCore/documentation/MapboxNavigationCore":{"type":"topic","identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore","title":"MapboxNavigationCore","url":"\/documentation\/mapboxnavigationcore","role":"collection","abstract":[],"kind":"symbol"},"doc://MapboxNavigationCore/documentation/MapboxNavigationCore/CoreConfig/RouteRequestConfig-swift.struct/allowsUTurnAtWaypoint":{"abstract":[{"text":"A Boolean value that indicates whether a returned route may require a point U-turn at an intermediate","type":"text"},{"text":" ","type":"text"},{"text":"waypoint.","type":"text"}],"kind":"symbol","identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct\/allowsUTurnAtWaypoint","fragments":[{"kind":"keyword","text":"var"},{"text":" ","kind":"text"},{"kind":"identifier","text":"allowsUTurnAtWaypoint"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:Sb","text":"Bool","kind":"typeIdentifier"}],"role":"symbol","title":"allowsUTurnAtWaypoint","type":"topic","url":"\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct\/allowsuturnatwaypoint"}}}