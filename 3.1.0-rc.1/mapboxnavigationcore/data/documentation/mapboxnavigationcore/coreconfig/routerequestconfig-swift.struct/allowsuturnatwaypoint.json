":"\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct","type":"topic","title":"CoreConfig.RouteRequestConfig","kind":"symbol","role":"symbol","fragments":[{"kind":"keyword","text":"struct"},{"text":" ","kind":"text"},{"text":"RouteRequestConfig","kind":"identifier"}]}}}