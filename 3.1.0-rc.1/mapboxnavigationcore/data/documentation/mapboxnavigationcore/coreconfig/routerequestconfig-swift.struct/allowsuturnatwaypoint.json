{"hierarchy":{"paths":[["doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore","doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig","doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct"]]},"metadata":{"title":"allowsUTurnAtWaypoint","symbolKind":"property","role":"symbol","modules":[{"name":"MapboxNavigationCore"}],"roleHeading":"Instance Property","fragments":[{"kind":"keyword","text":"var"},{"text":" ","kind":"text"},{"kind":"identifier","text":"allowsUTurnAtWaypoint"},{"kind":"text","text":": "},{"text":"Bool","kind":"typeIdentifier","preciseIdentifier":"s:Sb"}],"externalID":"s:20MapboxNavigationCore0C6ConfigV012RouteRequestD0V21allowsUTurnAtWaypointSbvp"},"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"allowsUTurnAtWaypoint"},{"text":": ","kind":"text"},{"text":"Bool","kind":"typeIdentifier","preciseIdentifier":"s:Sb"}],"languages":["swift"],"platforms":["iOS"]}]},{"content":[{"type":"heading","text":"Discussion","level":2,"anchor":"discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"If the value of this property is "},{"code":"true","type":"codeVoice"},{"type":"text","text":", a returned route may require an immediate U-turn at an"},{"type":"text","text":" "},{"type":"text","text":"intermediate"},{"text":" ","type":"text"},{"type":"text","text":"waypoint. At an intermediate waypoint, if the value of this property is "},{"type":"codeVoice","code":"false"},{"text":", each returned route may","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"continue straight ahead or turn to either side but may not U-turn. This property has no effect if only two"},{"type":"text","text":" "},{"type":"text","text":"waypoints are specified."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Set this property to "},{"type":"codeVoice","code":"true"},{"type":"text","text":" if you expect the user to traverse each leg of the trip separately. For"},{"text":" ","type":"text"},{"type":"text","text":"example, it"},{"type":"text","text":" "},{"text":"would be quite easy for the user to effectively “U-turn” at a waypoint if the user first parks the car and","type":"text"},{"text":" ","type":"text"},{"text":"patronizes a restaurant there before embarking on the next leg of the trip. Set this property to ","type":"text"},{"code":"false","type":"codeVoice"},{"text":" if","type":"text"},{"text":" ","type":"text"},{"text":"you","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"expect the user to proceed to the next waypoint immediately upon arrival. For example, if the user only"},{"type":"text","text":" "},{"type":"text","text":"needs to"},{"type":"text","text":" "},{"type":"text","text":"drop off a passenger or package at the waypoint before continuing, it would be inconvenient to perform a"},{"text":" ","type":"text"},{"text":"U-turn","type":"text"},{"type":"text","text":" "},{"type":"text","text":"at that location."},{"type":"text","text":" "},{"type":"text","text":"The default value of this property is `false."}]}],"kind":"content"}],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct\/allowsuturnatwaypoint"]}],"schemaVersion":{"major":0,"patch":0,"minor":3},"sections":[],"kind":"symbol","abstract":[{"text":"A Boolean value that indicates whether a returned route may require a point U-turn at an intermediate","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"waypoint."}],"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct\/allowsUTurnAtWaypoint"},"references":{"doc://MapboxNavigationCore/documentation/MapboxNavigationCore/CoreConfig":{"fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"CoreConfig"}],"navigatorTitle":[{"text":"CoreConfig","kind":"identifier"}],"title":"CoreConfig","role":"symbol","kind":"symbol","abstract":[{"type":"text","text":"Mutable Core SDK configuration."}],"identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig","url":"\/documentation\/mapboxnavigationcore\/coreconfig","type":"topic"},"doc://MapboxNavigationCore/documentation/MapboxNavigationCore/CoreConfig/RouteRequestConfig-swift.struct":{"abstract":[{"text":"Mutable Routing configuration.","type":"text"}],"kind":"symbol","fragments":[{"kind":"keyword","text":"struct"},{"text":" ","kind":"text"},{"text":"RouteRequestConfig","kind":"identifier"}],"navigatorTitle":[{"kind":"identifier","text":"RouteRequestConfig"}],"url":"\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct","role":"symbol","type":"topic","title":"CoreConfig.RouteRequestConfig","identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct"},"doc://MapboxNavigationCore/documentation/MapboxNavigationCore/CoreConfig/RouteRequestConfig-swift.struct/allowsUTurnAtWaypoint":{"abstract":[{"type":"text","text":"A Boolean value that indicates whether a returned route may require a point U-turn at an intermediate"},{"text":" ","type":"text"},{"type":"text","text":"waypoint."}],"type":"topic","url":"\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct\/allowsuturnatwaypoint","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"allowsUTurnAtWaypoint"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:Sb","text":"Bool","kind":"typeIdentifier"}],"role":"symbol","title":"allowsUTurnAtWaypoint","kind":"symbol","identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct\/allowsUTurnAtWaypoint"},"doc://MapboxNavigationCore/documentation/MapboxNavigationCore":{"title":"MapboxNavigationCore","abstract":[],"url":"\/documentation\/mapboxnavigationcore","kind":"symbol","role":"collection","identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore","type":"topic"}}}