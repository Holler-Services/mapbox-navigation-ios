{"hierarchy":{"paths":[["doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore","doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig","doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct"]]},"abstract":[{"text":"A Boolean value that indicates whether a returned route may require a point U-turn at an intermediate","type":"text"},{"text":" ","type":"text"},{"text":"waypoint.","type":"text"}],"metadata":{"modules":[{"name":"MapboxNavigationCore"}],"symbolKind":"property","title":"allowsUTurnAtWaypoint","role":"symbol","externalID":"s:20MapboxNavigationCore0C6ConfigV012RouteRequestD0V21allowsUTurnAtWaypointSbvp","roleHeading":"Instance Property","fragments":[{"text":"var","kind":"keyword"},{"kind":"text","text":" "},{"text":"allowsUTurnAtWaypoint","kind":"identifier"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:Sb","text":"Bool","kind":"typeIdentifier"}]},"schemaVersion":{"minor":3,"major":0,"patch":0},"kind":"symbol","sections":[],"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"text":"var","kind":"keyword"},{"kind":"text","text":" "},{"text":"allowsUTurnAtWaypoint","kind":"identifier"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:Sb","kind":"typeIdentifier","text":"Bool"}],"platforms":["iOS"],"languages":["swift"]}]},{"content":[{"level":2,"anchor":"discussion","type":"heading","text":"Discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"If the value of this property is "},{"code":"true","type":"codeVoice"},{"text":", a returned route may require an immediate U-turn at an","type":"text"},{"type":"text","text":" "},{"type":"text","text":"intermediate"},{"text":" ","type":"text"},{"type":"text","text":"waypoint. At an intermediate waypoint, if the value of this property is "},{"code":"false","type":"codeVoice"},{"type":"text","text":", each returned route may"},{"text":" ","type":"text"},{"type":"text","text":"continue straight ahead or turn to either side but may not U-turn. This property has no effect if only two"},{"text":" ","type":"text"},{"type":"text","text":"waypoints are specified."}]},{"inlineContent":[{"type":"text","text":"Set this property to "},{"type":"codeVoice","code":"true"},{"type":"text","text":" if you expect the user to traverse each leg of the trip separately. For"},{"type":"text","text":" "},{"type":"text","text":"example, it"},{"type":"text","text":" "},{"type":"text","text":"would be quite easy for the user to effectively “U-turn” at a waypoint if the user first parks the car and"},{"type":"text","text":" "},{"text":"patronizes a restaurant there before embarking on the next leg of the trip. Set this property to ","type":"text"},{"type":"codeVoice","code":"false"},{"type":"text","text":" if"},{"text":" ","type":"text"},{"type":"text","text":"you"},{"type":"text","text":" "},{"type":"text","text":"expect the user to proceed to the next waypoint immediately upon arrival. For example, if the user only"},{"type":"text","text":" "},{"type":"text","text":"needs to"},{"text":" ","type":"text"},{"text":"drop off a passenger or package at the waypoint before continuing, it would be inconvenient to perform a","type":"text"},{"type":"text","text":" "},{"type":"text","text":"U-turn"},{"text":" ","type":"text"},{"type":"text","text":"at that location."},{"text":" ","type":"text"},{"text":"The default value of this property is `false.","type":"text"}],"type":"paragraph"}],"kind":"content"}],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct\/allowsuturnatwaypoint"]}],"identifier":{"url":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct\/allowsUTurnAtWaypoint","interfaceLanguage":"swift"},"references":{"doc://MapboxNavigationCore/documentation/MapboxNavigationCore/CoreConfig":{"kind":"symbol","navigatorTitl