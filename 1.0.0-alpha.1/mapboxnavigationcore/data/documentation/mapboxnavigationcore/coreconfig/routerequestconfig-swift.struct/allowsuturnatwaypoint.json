{"hierarchy":{"paths":[["doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore","doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig","doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct"]]},"metadata":{"externalID":"s:20MapboxNavigationCore0C6ConfigV012RouteRequestD0V21allowsUTurnAtWaypointSbvp","role":"symbol","roleHeading":"Instance Property","title":"allowsUTurnAtWaypoint","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"text":"allowsUTurnAtWaypoint","kind":"identifier"},{"kind":"text","text":": "},{"kind":"typeIdentifier","preciseIdentifier":"s:Sb","text":"Bool"}],"symbolKind":"property","modules":[{"name":"MapboxNavigationCore"}]},"schemaVersion":{"minor":3,"patch":0,"major":0},"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct\/allowsuturnatwaypoint"]}],"sections":[],"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct\/allowsUTurnAtWaypoint"},"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"text":"allowsUTurnAtWaypoint","kind":"identifier"},{"text":": ","kind":"text"},{"text":"Bool","kind":"typeIdentifier","preciseIdentifier":"s:Sb"}],"languages":["swift"],"platforms":["iOS"]}]},{"kind":"content","content":[{"type":"heading","text":"Discussion","anchor":"discussion","level":2},{"inlineContent":[{"type":"text","text":"If the value of this property is "},{"type":"codeVoice","code":"true"},{"text":", a returned route may require an immediate U-turn at an","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"intermediate"},{"type":"text","text":" "},{"type":"text","text":"waypoint. At an intermediate waypoint, if the value of this property is "},{"type":"codeVoice","code":"false"},{"type":"text","text":", each returned route may"},{"text":" ","type":"text"},{"text":"continue straight ahead or turn to either side but may not U-turn. This property has no effect if only two","type":"text"},{"type":"text","text":" "},{"type":"text","text":"waypoints are specified."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Set this property to "},{"code":"true","type":"codeVoice"},{"type":"text","text":" if you expect the user to traverse each leg of the trip separately. For"},{"text":" ","type":"text"},{"text":"example, it","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"would be quite easy for the user to effectively “U-turn” at a waypoint if the user first parks the car and"},{"type":"text","text":" "},{"type":"text","text":"patronizes a restaurant there before embarking on the next leg of the trip. Set this property to "},{"type":"codeVoice","code":"false"},{"text":" if","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"you"},{"type":"text","text":" "},{"text":"expect the user to proceed to the next waypoint immediately upon arrival. For example, if the user only","type":"text"},{"text":" ","type":"text"},{"text":"needs to","type":"text"},{"text":" ","type":"text"},{"text":"drop off a passenger or package at the waypoint before continuing, it would be inconvenient to perform a","type":"text"},{"type":"text","text":" "},{"text":"U-turn","type":"text"},{"type":"text","text":" "},{"text":"at that location.","type":"text"},{"text":" ","type":"text"},{"text":"The default value of this property is `false.","type":"text"}]}]}],"abstract":[{"text":"A Boolean value that indicates whether a returned route may require a point U-turn at an intermediate","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"waypoint."}],"kind":"symbol","references":{"doc://MapboxNavigationCore/documentation/MapboxNavigationCore/CoreConfig":{"title":"CoreConfig","kind":"symbol","abstract":[{"type":"text","text":"Mutable Core SDK configuration."}],"identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig","role":"symbol","url":"\/documentation\/mapboxnavigationcore\/coreconfig","type":"topic","navigatorTitle":[{"text":"CoreConfig","kind":"identifier"}],"fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"CoreConfig","kind":"identifier"}]},"doc://MapboxNavigationCore/documentation/MapboxNavigationCore/CoreConfig/RouteRequestConfig-swift.struct":{"identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct","url":"\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct","role":"symbol","title":"CoreConfig.RouteRequestConfig","abstract":[{"text":"Mutable Routing configuration.","type":"text"}],"fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"RouteRequestConfig"}],"kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"RouteRequestConfig"}]},"doc://MapboxNavigationCore/documentation/MapboxNavigationCore":{"kind":"symbol","role":"collection","title":"MapboxNavigationCore","identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore","url":"\/documentation\/mapboxnavigationcore","abstract":[],"type":"topic"},"doc://MapboxNavigationCore/documentation/MapboxNavigationCore/CoreConfig/RouteRequestConfig-swift.struct/allowsUTurnAtWaypoint":{"fragments":[{"text":"var","kind":"keyword"},{"kind":"text","text":" "},{"text":"allowsUTurnAtWaypoint","kind":"identifier"},{"kind":"text","text":": "},{"text":"Bool","kind":"typeIdentifier","preciseIdentifier":"s:Sb"}],"title":"allowsUTurnAtWaypoint","url":"\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct\/allowsuturnatwaypoint","role":"symbol","identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct\/allowsUTurnAtWaypoint","abstract":[{"text":"A Boolean value that indicates whether a returned route may require a point U-turn at an intermediate","type":"text"},{"type":"text","text":" "},{"text":"waypoint.","type":"text"}],"kind":"symbol","type":"topic"}}}