{"hierarchy":{"paths":[["doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore","doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig","doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct"]]},"schemaVersion":{"patch":0,"major":0,"minor":3},"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct\/allowsUTurnAtWaypoint"},"metadata":{"title":"allowsUTurnAtWaypoint","symbolKind":"property","roleHeading":"Instance Property","externalID":"s:20MapboxNavigationCore0C6ConfigV012RouteRequestD0V21allowsUTurnAtWaypointSbvp","role":"symbol","fragments":[{"text":"var","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"allowsUTurnAtWaypoint"},{"kind":"text","text":": "},{"text":"Bool","kind":"typeIdentifier","preciseIdentifier":"s:Sb"}],"modules":[{"name":"MapboxNavigationCore"}]},"kind":"symbol","primaryContentSections":[{"declarations":[{"languages":["swift"],"tokens":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"allowsUTurnAtWaypoint","kind":"identifier"},{"text":": ","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"s:Sb","text":"Bool"}],"platforms":["iOS"]}],"kind":"declarations"},{"content":[{"level":2,"anchor":"discussion","text":"Discussion","type":"heading"},{"type":"paragraph","inlineContent":[{"text":"If the value of this property is ","type":"text"},{"type":"codeVoice","code":"true"},{"text":", a returned route may require an immediate U-turn at an","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"intermediate"},{"type":"text","text":" "},{"type":"text","text":"waypoint. At an intermediate waypoint, if the value of this property is "},{"type":"codeVoice","code":"false"},{"text":", each returned route may","type":"text"},{"text":" ","type":"text"},{"text":"continue straight ahead or turn to either side but may not U-turn. This property has no effect if only two","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"waypoints are specified."}]},{"inlineContent":[{"text":"Set this property to ","type":"text"},{"code":"true","type":"codeVoice"},{"type":"text","text":" if you expect the user to traverse each leg of the trip separately. For"},{"type":"text","text":" "},{"text":"example, it","type":"text"},{"type":"text","text":" "},{"type":"text","text":"would be quite easy for the user to effectively “U-turn” at a waypoint if the user first parks the car and"},{"text":" ","type":"text"},{"text":"patronizes a restaurant there before embarking on the next leg of the trip. Set this property to ","type":"text"},{"code":"false","type":"codeVoice"},{"type":"text","text":" if"},{"type":"text","text":" "},{"text":"you","type":"text"},{"text":" ","type":"text"},{"text":"expect the user to proceed to the next waypoint immediately upon arrival. For example, if the user only","type":"text"},{"text":" ","type":"text"},{"text":"needs to","type":"text"},{"text":" ","type":"text"},{"text":"drop off a passenger or package at the waypoint before continuing, it would be inconvenient to perform a","type":"text"},{"type":"text","text":" "},{"text":"U-turn","type":"text"},{"type":"text","text":" "},{"text":"at that location.","type":"text"},{"type":"text","text":" "},{"type":"text","text":"The default value of this property is `false."}],"type":"paragraph"}],"kind":"content"}],"variants":[{"paths":["\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct\/allowsuturnatwaypoint"],"traits":[{"interfaceLanguage":"swift"}]}],"abstract":[{"type":"text","text":"A Boolean value that indicates whether a returned route may require a point U-turn at an intermediate"},{"type":"text","text":" "},{"type":"text","text":"waypoint."}],"sections":[],"references":{"doc://MapboxNavigationCore/documentation/MapboxNavigationCore":{"title":"MapboxNavigationCore","identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore","role":"collection","type":"topic","abstract":[],"url":"\/documentation\/mapboxnavigationcore","kind":"symbol"},"doc://MapboxNavigationCore/documentation/MapboxNavigationCore/CoreConfig":{"type":"topic","kind":"symbol","identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig","role":"symbol","title":"CoreConfig","navigatorTitle":[{"text":"CoreConfig","kind":"identifier"}],"abstract":[{"type":"text","text":"Mutable Core SDK configuration."}],"fragments":[{"kind":"keyword","text":"struct"},{"text":" ","kind":"text"},{"text":"CoreConfig","kind":"identifier"}],"url":"\/documentation\/mapboxnavigationcore\/coreconfig"},"doc://MapboxNavigationCore/documentation/MapboxNavigationCore/CoreConfig/RouteRequestConfig-swift.struct":{"type":"topic","kind":"symbol","identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct","role":"symbol","title":"CoreConfig.RouteRequestConfig","navigatorTitle":[{"text":"RouteRequestConfig","kind":"identifier"}],"abstract":[{"type":"text","text":"Mutable Routing configuration."}],"fragments":[{"text":"struct","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"RouteRequestConfig"}],"url":"\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct"},"doc://MapboxNavigationCore/documentation/MapboxNavigationCore/CoreConfig/RouteRequestConfig-swift.struct/allowsUTurnAtWaypoint":{"fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"allowsUTurnAtWaypoint","kind":"identifier"},{"kind":"text","text":": "},{"kind":"typeIdentifier","preciseIdentifier":"s:Sb","text":"Bool"}],"url":"\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct\/allowsuturnatwaypoint","abstract":[{"type":"text","text":"A Boolean value that indicates whether a returned route may require a point U-turn at an intermediate"},{"type":"text","text":" "},{"text":"waypoint.","type":"text"}],"identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct\/allowsUTurnAtWaypoint","kind":"symbol","type":"topic","title":"allowsUTurnAtWaypoint","role":"symbol"}}}