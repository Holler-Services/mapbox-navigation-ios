ration."}]},"doc://MapboxNavigationCore/documentation/MapboxNavigationCore":{"title":"MapboxNavigationCore","abstract":[],"identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore","role":"collection","kind":"symbol","url":"\/documentation\/mapboxnavigationcore","type":"topic"}}}