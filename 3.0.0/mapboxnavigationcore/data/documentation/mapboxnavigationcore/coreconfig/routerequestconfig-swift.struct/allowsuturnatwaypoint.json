{"hierarchy":{"paths":[["doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore","doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig","doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct"]]},"metadata":{"symbolKind":"property","roleHeading":"Instance Property","externalID":"s:20MapboxNavigationCore0C6ConfigV012RouteRequestD0V21allowsUTurnAtWaypointSbvp","role":"symbol","title":"allowsUTurnAtWaypoint","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"allowsUTurnAtWaypoint","kind":"identifier"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:Sb","text":"Bool","kind":"typeIdentifier"}],"modules":[{"name":"MapboxNavigationCore"}]},"schemaVersion":{"major":0,"patch":0,"minor":3},"variants":[{"paths":["\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct\/allowsuturnatwaypoint"],"traits":[{"interfaceLanguage":"swift"}]}],"sections":[],"kind":"symbol","abstract":[{"type":"text","text":"A Boolean value that indicates whether a returned route may require a point U-turn at an intermediate"},{"type":"text","text":" "},{"text":"waypoint.","type":"text"}],"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct\/allowsUTurnAtWaypoint"},"primaryContentSections":[{"kind":"declarations","declarations":[{"languages":["swift"],"platforms":["iOS"],"tokens":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"allowsUTurnAtWaypoint"},{"kind":"text","text":": "},{"preciseIdentifier":"s:Sb","kind":"typeIdentifier","text":"Bool"}]}]},{"kind":"content","content":[{"level":2,"type":"heading","text":"Discussion","anchor":"discussion"},{"inlineContent":[{"type":"text","text":"If the value of this property is "},{"type":"codeVoice","code":"true"},{"type":"text","text":", a returned route may require an immediate U-turn at an"},{"type":"text","text":" "},{"text":"intermediate","type":"text"},{"type":"text","text":" "},{"type":"text","text":"waypoint. At an intermediate waypoint, if the value of this property is "},{"code":"false","type":"codeVoice"},{"type":"text","text":", each returned route may"},{"type":"text","text":" "},{"text":"continue straight ahead or turn to either side but may not U-turn. This property has no effect if only two","type":"text"},{"text":" ","type":"text"},{"text":"waypoints are specified.","type":"text"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Set this property to "},{"code":"true","type":"codeVoice"},{"type":"text","text":" if you expect the user to traverse each leg of the trip separately. For"},{"type":"text","text":" "},{"type":"text","text":"example, it"},{"type":"text","text":" "},{"text":"would be quite easy for the user to effectively “U-turn” at a waypoint if the user first parks the car and","type":"text"},{"text":" ","type":"text"},{"text":"patronizes a restaurant there before embarking on the next leg of the trip. Set this property to ","type":"text"},{"type":"codeVoice","code":"false"},{"text":" if","type":"text"},{"text":" ","type":"text"},{"text":"you","type":"text"},{"type":"text","text":" "},{"text":"expect the user to proceed to the next waypoint immediately upon arrival. For example, if the user only","type":"text"},{"type":"text","text":" "},{"text":"needs to","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"drop off a passenger or package at the waypoint before continuing, it would be inconvenient to perform a"},{"text":" ","type":"text"},{"text":"U-turn","type":"text"},{"text":" ","type":"text"},{"text":"at that location.","type":"text"},{"type":"text","text":" "},{"text":"The default value of this property is `false.","type":"text"}]}]}],"references":{"doc://MapboxNavigationCore/documentation/MapboxNavigationCore/CoreConfig/RouteRequestConfig-swift.struct":{"type":"topic","title":"CoreConfig.RouteRequestConfig","navigatorTitle":[{"text":"RouteRequestConfig","kind":"identifier"}],"abstract":[{"text":"Mutable Routing configuration.","type":"text"}],"identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct","url":"\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct","role":"symbol","kind":"symbol","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"text":"RouteRequestConfig","kind":"identifier"}]},"doc://MapboxNavigationCore/documentation/MapboxNavigationCore/CoreConfig/RouteRequestConfig-swift.struct/allowsUTurnAtWaypoint":{"url":"\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct\/allowsuturnatwaypoint","abstract":[{"type":"text","text":"A Boolean value that indicates whether a returned route may require a point U-turn at an intermediate"},{"type":"text","text":" "},{"text":"waypoint.","type":"text"}],"type":"topic","identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct\/allowsUTurnAtWaypoint","fragments":[{"kind":"keyword","text":"var"},{"text":" ","kind":"text"},{"kind":"identifier","text":"allowsUTurnAtWaypoint"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:Sb","kind":"typeIdentifier","text":"Bool"}],"role":"symbol","title":"allowsUTurnAtWaypoint","kind":"symbol"},"doc://MapboxNavigationCore/documentation/MapboxNavigationCore/CoreConfig":{"kind":"symbol","type":"topic","abstract":[{"type":"text","text":"Mutable Core SDK configuration."}],"title":"CoreConfig","navigatorTitle":[{"kind":"identifier","text":"CoreConfig"}],"identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"CoreConfig","kind":"identifier"}],"url":"\/documentation\/mapboxnavigationcore\/coreconfig","role":"symbol"},"doc://MapboxNavigationCore/documentation/MapboxNavigationCore":{"type":"topic","url":"\/documentation\/mapboxnavigationcore","kind":"symbol","title":"MapboxNavigationCore","identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore","role":"collection","abstract":[]}}}